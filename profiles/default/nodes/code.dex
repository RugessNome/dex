
import node;

class CodeBlock : Node
{
public:
  Ref<GroupNode> nodes;
  String lang;
public:
  CodeBlock()
  {
    nodes = Ref<GroupNode>::make();
  }
  
  ~CodeBlock() = default;
  
    
  void add(const Ref<Node> & n)
  {
    nodes.get().push_back(n);
  }
};

class InlineCode : SingleArgumentNode
{
public:
  InlineCode(const Ref<Node> & text)
    : SingleArgumentNode(text)
  {

  }
  
  ~InlineCode() = default;
};
